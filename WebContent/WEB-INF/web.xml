<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Twitter-Sign-In-Web</display-name>
  <servlet>
    <description></description>
    <display-name>TwitterSigninServlet</display-name>
    <servlet-name>TwitterSigninServlet</servlet-name>
    <servlet-class>com.tap.TwitterSigninServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>Callback</display-name>
    <servlet-name>Callback</servlet-name>
    <servlet-class>com.tap.Callback</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>ProcessFof</display-name>
    <servlet-name>ProcessFof</servlet-name>
    <servlet-class>com.tap.ProcessFof</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TwitterSigninServlet</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Callback</servlet-name>
    <url-pattern>/Callback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessFof</servlet-name>
    <url-pattern>/ProcessFof</url-pattern>
  </servlet-mapping>
</web-app>